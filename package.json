  "devDependencies": {
    "@faker-js/faker": "8.1.0",
    "@innei-util/eslint-config-react-ts": "0.2.1",
    "@innei-util/prettier": "0.8.2",
    "@types/node": "18.16.14",
    "@types/react": "^18.0.15",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2",
    "autoprefixer": "10.4.7",
    "cypress": "^13.6.0",
    "eslint": "8.20.0",
    "husky": "8.0.1",
    "lint-staged": "13.2.2",
    "postcss": "8.4.14",
    "postcss-import": "14.1.0",
    "prettier": "3.0.3",
